{
    "name": "cuongdv/base-repository",
    "authors": [
        {
            "name": "Cuong Dang",
            "email": "cdang@softel.vn"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Softel\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestFunctions.php"
        ],
        "psr-4": {
            "Softel\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Softel\\RepositoryServiceProvider"
            ]
        }
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    }
}